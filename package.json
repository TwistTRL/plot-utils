{
  "name": "plot-utils",
  "version": "0.1.0",
  "description": "Assorted plotting functions and utils designed to work in React framework.",
  "main": "dist/index.js",
  "author": "Lingyu Zhou (zhoulytwin@gmail.com)",
  "license": "MIT",
  "dependencies": {
